<fieldset class="player-add">
    <%= f.hidden_field :_destroy, class: "removable" %>
    <%= f.hidden_field :team_id, :value => @team.id %>
    <%= f.select :user_id, User.all.collect{ |p| [p.user_name, p.id]}, {}, {class: "select-player"} %>
    <%= link_to "<img src='https://feathericons.com/node_modules/feather-icons/dist/icons/delete.svg'>".html_safe, '#', class: "remove_players" %>
</fieldset>